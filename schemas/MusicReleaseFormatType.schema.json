{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://schema.org/MusicReleaseFormatType",
  "title": "MusicReleaseFormatType",
  "description": "Format of this release (the type of recording media used, ie. compact disc, digital media, LP, etc.).",
  "type": "object",
  "format": "http://schema.org/MusicReleaseFormatType",
  "oneOf": [
    { "description": "CDFormat.", "const": "CDFormat" },
    { "description": "CassetteFormat.", "const": "CassetteFormat" },
    { "description": "DVDFormat.", "const": "DVDFormat" },
    {
      "description": "DigitalAudioTapeFormat.",
      "const": "DigitalAudioTapeFormat"
    },
    { "description": "DigitalFormat.", "const": "DigitalFormat" },
    { "description": "LaserDiscFormat.", "const": "LaserDiscFormat" },
    { "description": "VinylFormat.", "const": "VinylFormat" }
  ],
  "properties": {
    "@context": { "const": "https://schema.org" },
    "@type": { "const": "MusicReleaseFormatType" }
  },
  "required": ["@type"]
}
