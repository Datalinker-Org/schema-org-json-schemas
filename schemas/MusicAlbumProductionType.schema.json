    { "description": "DemoAlbum.", "const": "DemoAlbum" },
    { "description": "SpokenWordAlbum.", "const": "SpokenWordAlbum" },
    { "description": "CompilationAlbum.", "const": "CompilationAlbum" },
    { "description": "StudioAlbum.", "const": "StudioAlbum" },
