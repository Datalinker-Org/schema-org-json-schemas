{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://schema.org/QuantitativeValueDistribution",
  "title": "QuantitativeValueDistribution",
  "description": "A statistical distribution of values.",
  "type": "object",
  "format": "http://schema.org/QuantitativeValueDistribution",
  "allOf": [
    {
      "description": "Structured values are used when the value of a property has a more complex structure than simply being a textual value or a reference to another thing.",
      "$ref": "StructuredValue.schema.json"
    }
  ],
  "properties": {
    "duration": {
      "description": "The duration of the item (movie, audio recording, event, etc.) in ISO 8601 date format.",
      "type": "string",
      "format": "https://schema.org/Duration"
    },
    "median": { "description": "The median value.", "type": "number" },
    "percentile10": {
      "description": "The 10th percentile value.",
      "type": "number"
    },
    "percentile25": {
      "description": "The 25th percentile value.",
      "type": "number"
    },
    "percentile75": {
      "description": "The 75th percentile value.",
      "type": "number"
    },
    "percentile90": {
      "description": "The 90th percentile value.",
      "type": "number"
    }
  }
}
