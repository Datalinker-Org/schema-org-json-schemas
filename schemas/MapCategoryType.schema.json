{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://schema.org/MapCategoryType",
  "title": "MapCategoryType",
  "description": "An enumeration of several kinds of Map.",
  "type": "object",
  "format": "http://schema.org/MapCategoryType",
  "oneOf": [
    { "description": "A parking map.", "const": "ParkingMap" },
    { "description": "A seating map.", "const": "SeatingMap" },
    { "description": "A transit map.", "const": "TransitMap" },
    {
      "description": "A venue map (e.g. for malls, auditoriums, museums, etc.).",
      "const": "VenueMap"
    }
  ],
  "properties": {
    "@context": { "const": "https://schema.org" },
    "@type": { "const": "MapCategoryType" }
  },
  "required": ["@type"]
}
